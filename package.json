{
  "name": "opensea-push-client",
  "version": "1.0.0",
  "description": "An SDK to receive pushed updates from OpenSea over websocket",
  "license": "MIT",
  "author": "OpenSea Developers",
  "repository": "https://github.com/ProjectOpenSea/pushed-updates.git",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "eslint": "eslint . --max-warnings 0 --ext .js,.ts --fix",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "prettier:check:package.json": "yarn prettier-package-json --list-different",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "phoenix": "^1.6.2",
    "ws": "^8.3.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/phoenix": "^1.5.3",
    "@types/sinon": "^10.0.6",
    "@types/ws": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "chai": "^4.3.4",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jsdom": "^18.0.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.3.4",
    "microbundle": "^0.14.2",
    "mocha": "^9.1.3",
    "mocha-typescript": "^1.1.17",
    "mock-socket": "^9.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "prettier-package-json": "^2.6.3",
    "sinon": "^12.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write"
    ],
    "**/*.{ts,tsx,js,jsx,html,md,mdx,yml,json}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix"
    ]
  },
  "source": "src/index.ts",
  "unpkg": "./dist/index.umd.js"
}
